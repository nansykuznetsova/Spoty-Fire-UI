{
  "name": "spoty-fire-ui",
  "version": "1.0.6",
  "description": "A simple react component collection",
  "main": "./build/src/index.js",
  "scripts": {
    "test": "jest --silent",
    "build": "rollup -c",
    "build-storybook": "storybook build",
    "format": "prettier --write src/**/*.{js,jsx,css} --config ./.prettierrc",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,js}'",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "storybook": "storybook dev -p 6006",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nansykuznetsova/Spoty-Fire-UI.git"
  },
  "author": "Anastasiia Kuznetsova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nansykuznetsova/Spoty-Fire-UI/issues"
  },
  "homepage": "https://github.com/nansykuznetsova/Spoty-Fire-UI#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-vite": "^7.0.12",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "global-jsdom": "^9.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "postcss-modules": "^6.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^7.0.12"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "files": [
    "build"
  ]
}
